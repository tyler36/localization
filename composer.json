{
    "name": "tyler36/localization",
    "keywords": ["laravel", "language", "locale", "middleware"],
    "license": "MIT",
    "description": "Localization middleware",
    "autoload": {
        "psr-4": {
            "Tyler36\\Localization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tyler36\\Localization\\LocalizationServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": [
            "@phpfix",
            "@phpcs"
        ],
        "phpcs": "phpcs -p --standard=phpcs.xml",
        "phpfix": "php-cs-fixer fix --show-progress=dots --allow-risky=yes"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "squizlabs/php_codesniffer": "^3.3"
    }
}
